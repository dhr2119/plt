int main() {
    int i;
    int j;
    matrix int [2,3] mi;
    matrix float [3,2] mf;
    mi = [1,2,3;4,5,6];
    mf = [1.3,2.2; 3.1,4.6; 5.2,6.5];
    for(i=0; i < mi:rows; ++i) {
        for(j=0; j < mi:cols; ++j) {
            print_int(mi[i,j]);
        }
        print_string("");
    }
    for(i=0; i < mf:rows; ++i) {
        for(j=0; j < mf:cols; ++j) {
            print_float(mf[i,j]);
        }
        print_string("");
    }
    return 0;
}